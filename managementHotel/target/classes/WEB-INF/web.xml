<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>

    <context-param>
        <param-name>WEB_SERVER</param-name>
        <param-value>localhost:8084/managementHotel_war_exploded/</param-value>
    </context-param>
    <context-param>
        <param-name>WEB_SERVER_ADMIN</param-name>
        <param-value>localhost:8084/managementHotel_war_exploded/admin/</param-value>
    </context-param>
    <servlet>
        <servlet-name>Home</servlet-name>
        <servlet-class>servlet.Home</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Admin</servlet-name>
        <servlet-class>servlet.Admin.Admin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AuthenticationAdmin</servlet-name>
        <servlet-class>servlet.Admin.AuthenticationAdmin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servlet.SettingsHR</servlet-name>
        <servlet-class>servlet.Admin.ServletSettingsHR</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertEmployee</servlet-name>
        <servlet-class>servlet.Admin.InsertEmployee</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HomeEmployee</servlet-name>
        <servlet-class>servlet.Employee.HomeEmployee</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginEmployee_V2</servlet-name>
        <servlet-class>servlet.Employee.LoginEmployee_V2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AjaxChangeLanguage</servlet-name>
        <servlet-class>servlet.Ajax.AjaxChangeLanguage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PaymentLoadView</servlet-name>
        <servlet-class>servlet.PaymentLoadView</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogOut</servlet-name>
        <servlet-class>servlet.Admin.LogOut</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>AjaxPaymentContinue</servlet-name>
        <servlet-class>servlet.Ajax.AjaxPaymentContinue</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AjaxPaymentEndPoint</servlet-name>
        <servlet-class>servlet.Ajax.AjaxPaymentEndPoint</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertHotelManager</servlet-name>
        <servlet-class>servlet.Admin.InsertHotelManager</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SettingManagerHotel</servlet-name>
        <servlet-class>servlet.Admin.SettingManagerHotel</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SettingManagerHotel</servlet-name>
        <url-pattern>/SettingManagerHotel</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertHotelManager</servlet-name>
        <url-pattern>/InsertHotelManager</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AjaxPaymentEndPoint</servlet-name>
        <url-pattern>/AjaxPaymentEndPoint</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AjaxPaymentContinue</servlet-name>
        <url-pattern>/AjaxPaymentContinue</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogOut</servlet-name>
        <url-pattern>/LogOut</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PaymentLoadView</servlet-name>
        <url-pattern>/PaymentLoadView</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AjaxChangeLanguage</servlet-name>
        <url-pattern>/AjaxChangeLanguage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginEmployee_V2</servlet-name>
        <url-pattern>/LoginEmployee</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertEmployee</servlet-name>
        <url-pattern>/InsertEmployee</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HomeEmployee</servlet-name>
        <url-pattern>/HomeEmployee</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet.SettingsHR</servlet-name>
        <url-pattern>/HR</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AuthenticationAdmin</servlet-name>
        <url-pattern>/AuthenticationAdmin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Admin</servlet-name>
        <url-pattern>/Admin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Home</servlet-name>
        <url-pattern>/Home</url-pattern>
    </servlet-mapping>
    <filter>
        <filter-name>UrlRewriteFilter</filter-name>
        <filter-class>org.tuckey.web.filters.urlrewrite.UrlRewriteFilter</filter-class>
        <init-param>
            <param-name>statusEnabledOnHosts</param-name>
            <param-value>localhost, dev.*.myco.com, *.uat.mycom.com</param-value>
        </init-param>
        <init-param>
            <param-name>requestEncoding</param-name>
            <param-value>UTF-8</param-value>
        </init-param>
    </filter>
    <filter-mapping>
        <filter-name>UrlRewriteFilter</filter-name>
        <url-pattern>/*</url-pattern>
        <dispatcher>REQUEST</dispatcher>
        <dispatcher>FORWARD</dispatcher>

    </filter-mapping>
</web-app>
